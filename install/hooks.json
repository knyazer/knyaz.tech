[
  {
    "id": "redeploy",
    "execute-command": "/bin/sh",
    "pass-arguments-to-command": [
    {
      "source": "string",
      "name": "/home/opc/server/redeploy.sh"
    }],
    "command-working-directory": "",
    "response-message": "Redeploying server.",
    "trigger-rule":
    {
      "match":
      {
        "type": "value",
        "value": "{WEBHOOK_ACCESS_TOKEN}",
        "parameter":
        {
          "source": "url",
          "name": "token"
        }
      }
    }
  }
]